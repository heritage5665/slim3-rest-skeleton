{
    "name": "moritz-h/slim3-rest-skeleton",
    "description": "Simple Slim Framework 3 skeleton for REST API.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton", "rest"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        },
        {
            "name": "Moritz Heinemann",
            "email": "mo@blog-srv.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src",
            "Database\\": "db"
        }
    },
    "require": {
        "slim/slim": "^3.5",
        "monolog/monolog": "^1.13",
        "illuminate/database": "^5.3",
        "robmorgan/phinx": "v0.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mockery/mockery": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "IntegrationTests\\": "tests/IntegrationTests",
            "UnitTests\\": "tests/UnitTests"
        }
    }
}
